{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/src/index.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  }
}